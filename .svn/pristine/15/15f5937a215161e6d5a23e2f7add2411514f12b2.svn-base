/*수정 삭제 버튼 */
#editBox {
	display: flex;
	justify-content :center;
	align-items: flex-end;
	margin-top: 30px;
	
}

.cancel {
	width: 120px;
	height: 40px;
	border: 1px solid #B5B5B5;
	color: #232323;
	background: white;
	border-radius: 7px;
	font-size: 16px;
	font-weight: 400;
	transition: all 0.3s ease 0s;
}

.cancel:hover {
	background: #ECECEC;
	border: 1px solid #B5B5B5;
}

#savebtn {
	background: white;
	color: #24D59E;
	border: 1px solid #24D59E;
	width: 120px;
	height: 40px;
	border-radius: 7px;
	font-size: 16px;
	font-weight: 400;
	margin-left: 25px;
	transition: all 0.3s ease 0s;
}

#savebtn:hover {
	background-color: #24D59E;
	color: white;
}