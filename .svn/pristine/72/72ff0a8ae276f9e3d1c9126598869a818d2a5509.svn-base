<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<link rel="stylesheet" href="<%=request.getContextPath() %>/resources/css/enter/entaddmem.css" />
<br>
<div class="all">
	<h2>멤버 조회</h2>
    <div class="container1">
    	<div style="margin-top: 50px; background: #D9D9D9;">
    		<h4 style="margin-left: 20px;margin-top: 20px;">멤버 초대</h4>
	    	<div class="member-form">
	    		<div class="form-row">
		            <div class="form-label">
		                <label for="name">아이디 <span class="required">필수</span></label>
		            </div>
		            <input type="text" id="mbrId" name="mbrId" class="form-input" placeholder="아이디 입력" />
		        </div>
		        <div class="form-row">
		            <div class="form-label">
		                <label for="name">이름 <span class="required">필수</span></label>
		            </div>
		            <input type="text" id="name" class="form-input" placeholder="이름 입력" />
		        </div>
				
		        <div class="form-row">
		            <div class="form-label">
		                <label for="email">이메일 <span class="required">필수</span></label>
		            </div>
		            <input type="email" id="email" class="form-input" placeholder="이메일 입력" />
		        </div>
		
		        <div class="form-row">
		            <div class="form-label">
		                <label for="phone">휴대폰</label>
		            </div>
		            <input type="tel" id="phone" class="form-input" placeholder="휴대폰번호 입력" />
		        </div>
		        <div class="form-row">
		            <button type="submit" class="submit-button">초대하기</button>
		        </div>
	        </div>
        </div>
    </div>
    <div>
    	
    </div>
</div>
