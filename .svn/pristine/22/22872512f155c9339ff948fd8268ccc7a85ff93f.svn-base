			    <foreach item="srvcTeamscaleCd" index="index" collection="srvcTeamscaleCdList" open="(" separator=", " close=")">
			        #{srvcTeamscaleCd}
			    </foreach>
			</if>
	</select>
	
	
	<!-- 자기소개서 제작 목록  -->
	<select id="getSIList" parameterType="hashMap" resultType="outsouVO">
		 WITH T AS
