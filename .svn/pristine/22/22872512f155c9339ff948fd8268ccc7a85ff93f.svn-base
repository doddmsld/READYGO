<!--  /////////// 검색 결과  ////////////////// -->	
<!-- 각 카테고리 별 목록  -->
	<select id="getscarchList" parameterType="hashMap" resultType="outsouVO">
		WITH T AS
	    (
	    	SELECT ROW_NUMBER() OVER(ORDER BY TO_NUMBER(S.OUTORD_NO) DESC) RNUM, S.*
	    	FROM
	    	(
		       SELECT OU.OUTORD_NO, 
		              OU.OUTORD_TTL,
		              OU.OUTORD_AMT,
		              (SELECT F.FILE_PATH_NM
		                 FROM FILE_DT F
		                WHERE F.FILE_GROUP_SN = OU.OUTORD_MAIN_FILE
		                  AND ROWNUM = 1) AS OUTORD_MAIN_FILE,
		               OU.OUTORD_WRTDE,OUTORD_UPDDE, OU.OUTORD_DEL_YN, OU.OUTORD_RDCNT, OU.MBR_ID,
		               (SELECT LISTAGG(OSK.KWRD_NM, ', ') WITHIN GROUP (ORDER BY OSK.KWRD_NM)
				          FROM OS_KEYWOED OSK
				         WHERE OSK.OUTORD_NO = OU.OUTORD_NO) AS KWRD_NM
		            FROM OUTSOU OU
			       WHERE 1 = 1
			         AND OU.OUTORD_DEL_YN = 'N'      -- 삭제한 항목은 나타나지 않음         
			) S
			WHERE 1 = 1
			AND   S.OUTORD_TTL LIKE '%'||#{keyword}||'%' OR S.KWRD_NM LIKE '%'||#{keyword}||'%'
	    )
	    SELECT * 
	      FROM T
	    WHERE T.RNUM BETWEEN (#{currentPage} - 1) * 10 + 1 AND #{currentPage} * 10
	</select>
	
	<!-- 전체 행의 수 -->
	<select id="getscarchTotal" parameterType="hashMap" resultType="int">
		SELECT COUNT(*)
    	FROM
    	(
	       SELECT ROW_NUMBER() OVER(ORDER BY TO_NUMBER(OU.OUTORD_NO) DESC) RNUM, 
	              OU.OUTORD_NO, 
	              OU.OUTORD_TTL,
	              OU.OUTORD_AMT,
	              (SELECT F.FILE_PATH_NM
	                 FROM FILE_DT F
	                WHERE F.FILE_GROUP_SN = OU.OUTORD_MAIN_FILE
	                  AND ROWNUM = 1) AS OUTORD_MAIN_FILE,
	               OU.OUTORD_WRTDE,OUTORD_UPDDE, OU.OUTORD_DEL_YN, OU.OUTORD_RDCNT, OU.MBR_ID,
	               (SELECT LISTAGG(OSK.KWRD_NM, ', ') WITHIN GROUP (ORDER BY OSK.KWRD_NM)
			          FROM OS_KEYWOED OSK
			         WHERE OSK.OUTORD_NO = OU.OUTORD_NO) AS KWRD_NM
	            FROM OUTSOU OU
		       WHERE 1 = 1
		         AND OU.OUTORD_DEL_YN = 'N'               
		) S
		WHERE 1 = 1
		AND   S.OUTORD_TTL LIKE '%'||#{keyword}||'%' OR S.KWRD_NM LIKE '%'||#{keyword}||'%'
	</select>
	
	
	
	<!--  리뷰 조회수 놓은거 3개  -->
	<select id="reviewBest"  resultType="boardVO">
		SELECT *
		FROM (
		    SELECT A.PST_SN, A.SE_NO, A.PST_TTL, A.MBR_ID,
		           TO_CHAR(A.PST_WRT_DT, 'YYYY.MM.DD') AS PST_WRT_DT, 
		           A.PST_MDFCN_DT, A.PST_DEL_YN, A.PST_INQ_CNT,
		           A.ETYMANMTTR_SN, A.PST_GOOD, A.PST_BAD,
		           B.BBS_NM,
		           OU.OUTORD_NO, OU.OUTORD_TTL,
		           (SELECT F.FILE_PATH_NM
		            FROM FILE_DT F
		            WHERE F.FILE_GROUP_SN = OU.OUTORD_MAIN_FILE
		            AND ROWNUM = 1) AS OUTORD_MAIN_FILE,
		           TO_CHAR(TO_DATE(OSA.OS_APLY_YMD, 'YYYY/MM/DD'), 'YYYY.MM.DD') AS OS_APLY_YMD,
		           OSA.OUTORD_STLM_AMT
		    FROM BOARD A
		    JOIN BOARD_TYPE B ON A.SE_NO = B.SE_NO
		    LEFT JOIN OS_APLY OSA ON A.MBR_ID = OSA.MBR_ID
		    LEFT JOIN OUTSOU OU ON OSA.OUTORD_NO = OU.OUTORD_NO
		    WHERE A.SE_NO = '5' 
		    AND A.PST_DEL_YN = '1'
		    ORDER BY A.PST_INQ_CNT DESC
		)
		<![CDATA[
		WHERE ROWNUM <= 3
		]]>
	</select>
