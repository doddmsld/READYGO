/* 모달 배경 */
.modal {
	display: none; /* 기본적으로 숨김 */
	position: fixed;
	z-index: 10000;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	overflow: auto;
	background-color: rgba(0, 0, 0, 0.4); /* 배경 투명도 */
}

/* 모달 콘텐츠 */
.modal-content {
	background-color: #fefefe;
	margin: 5% auto; /* 상단에서 약간의 간격을 주고 중앙 정렬 */
	padding: 35px;
	border: 1px solid #888;
	width: 40%; /* 모달 창의 너비를 줄입니다 (기본값은 80%) */
	max-height: 80%; /* 모달 창의 최대 높이 설정 */
	box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0
		rgba(0, 0, 0, 0.19); /* 그림자 효과 */
}

/* 닫기 버튼 스타일 */
.close {
	color: #aaa;
	float: right;
	font-size: 28px;
	font-weight: bold;
}

.close:hover, .close:focus {
	color: black;
	text-decoration: none;
	cursor: pointer;
}

.close:hover, .close:focus {
	color: black;
	text-decoration: none;
	cursor: pointer;
}
/* 폼 내 각 요소 간의 간격을 늘려줍니다 */
.modal-content form div {
	margin-bottom: 15px; /* 각 입력 필드 간 간격 */
}

/* 입력 필드의 높이를 늘립니다 */
#title, #content, #jobPost {
	width: 100%;
	padding: 10px;
	font-size: 16px;
	box-sizing: border-box; /* padding과 border를 포함한 크기 조정 */
	border: 1px solid #B5B5B5;
	border-radius: 7px;
}

#content {
	height: 150px; /* 텍스트 에어리어 높이 증가 */
	resize: vertical; /* 세로 크기 조절 가능 */
}

#file {
	display: block;
}

/* 버튼 스타일 조정 */
#cancel-btn, #scoutForm button[type="submit"] {
	padding: 10px 0;
	font-size: 16px;
	cursor: pointer;
	width: 200px;
	height: 50px;
	border-radius: 7px;
}

/* 모달 내 버튼 컨테이너 정렬 */
#scoutForm div:last-child {
	display: flex;
	justify-content: space-between; /* 버튼을 양쪽에 배치 */
}
