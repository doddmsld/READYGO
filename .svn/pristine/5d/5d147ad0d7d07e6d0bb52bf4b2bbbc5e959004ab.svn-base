@charset "UTF-8";

* {
    font-family: pretendard;
    color : #232323;
}
div#nav {
    text-align: center;
}
.container {
    margin-top: 30px;
}
.row{
	display: flex;
    flex-wrap: nowrap;
    margin-left: -105px;
}
.col-left {
    margin: 0px 33px 0px 50px;
}
.img {
    border-radius: 999px;
    height: 142px;
    width: 142px;
    object-fit: cover;
    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.3);
}
section.content {
    background: #f1f1f1;
}

/* 회원이름 */
#mbrNm{
	font-size : 20px;
	font-weight : 700;
	text-align : center;
	color : #232323;
}
/* 회원 상단 프로필 소개 */
#introduce p{
	text-align : center;
	font-size : 14px;
	color : #232323;
}
div#introduce {
    margin: 30px 0px 20px 0px;
}
#introduce2 p{
	font-size : 14px;
	color : #232323;
	margin-left: 330px;
}
#introduce2 {
    padding-bottom: 25px;
}
hr{
	width : 1250px;
}

/* 프로필 내용 */
.content-wrapper{
	background: #f1f1f1;
}
#topPro{
	background:#ffffff;
}
.content-wrapper>.content {
    padding: 0px -0px;
}
.container-fluid {
    width: 100%;
    padding-right: 0;
    padding-left: 0;
    margin-right: auto;
    margin-left: auto;
}

