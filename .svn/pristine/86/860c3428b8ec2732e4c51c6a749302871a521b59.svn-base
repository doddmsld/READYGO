@charset "UTF-8";

* {
	font-family: pretendard;
}

h5 {
	color: #232323;
}
h2{
	color:#232323;
	font-weight: 600;
}

#h3{
	margin : 50px 0 30px -30px;
	font-weight : 700;
	font-size : 30px;
	color : #212529;
}
#fmenu{
	margin-bottom: 10px;
}
.fc .fc-button-primary {
    background-color: white;
    opacity: 0.7;
    border: none;
    color: #232323;
    border: 1px solid #c9c7c7;
}
.fc .fc-button-primary:hover {
    background-color: #24D59E;
    border-color: var(--fc-button-hover-border-color);
    color: var(--fc-button-text-color);
}
#maintop {
	display: flex;
	justify-content: space-between;
	margin-bottom: 20px;
}

#mainright {
	height: 500px;
	display: flex;
	flex-direction: column;
}
#mainleft{
	height: 500px;
}

/*왼쪽 스타일*/
.header-area.header-sticky {
	min-height: 0px;
}
.fc .fc-button-primary:disabled {
    background-color: #24D59E;
    border-color: #24D59E;
    color: var(--fc-button-text-color);
}
.fc-daygrid-day-number{
	font-size:12px;
}
#calendar {
	width: 680px;
	height: auto;
}

#yrModal {
	position: fixed;
	left: 0;
    top: 0;
	width: 100%;
    height: 100%;
	background-color: rgba(41, 41, 41, 0.67);
	display: none;
	z-index: 1100;
}

#cont {
	display : block;
	margin: 175px auto;
	width: 16%;
	height: 55%;
	background-color: white;
	color: rgba(35, 35, 35, 1);
	border-radius: 15px;
}

#cont h4{
	float: left;
    padding: 20px 40px;
}

#closeModal {
	text-align: right;
	margin-right: 50px;
	margin-top: 30px;
}

.close {
	display:inline-block;
	*display:inline;
	color: rgba(35, 35, 35, 1);
	background: white;
	border-radius: 3px;
	width: 0px;
	height : 10px;
	margin-top: 18px;
	}
.close:after {
	display: inline-block;
	content: "\00d7";
	font-size:20pt;
}


#ModalContent {
	display: block;
 	text-align: justify;
	padding-left: 35px;
    line-height: 40px;
    height: 82%;
}
.flexDiv{
	display :flex;
	height: 100px;
}

#add {
	border: 1px solid #24D59E;
	border-radius: 5px;
	height: 43px;
    width: 100px;
	background: white;
	color: #24D59E;
	transition: all 0.3s ease 0s;
}

#add:hover {
	background: #24D59E;
	color: white;
}

#Caltitle{
	margin-right: 103px;
    margin-top: -5px;
}
#schTitle{
	margin-right: 406px;
    margin-top: -5px;
    height: 35px;
    width: 250px;
    border: 1px solid #ddd;
    border-radius: 5px;
    color: #757575;
}
.blockDiv{
	width : 175px;
	display :block;
}
#start{
 	margin-right: 50px;
}
#allDay{
	border: 1px solid #ddd;
	margin : 20px 0;
}
#schStart,#schEnd{
	width: 250px;
	height: 35px;
	border: 1px solid #ddd;
    border-radius: 5px;
    color: #757575;
    padding: 13px;
}
#back, #font{
	margin-right: 10px;
}
#schBColor{
	margin-right: 45px;
}
#schFColor{

}
.colorCss{
	width : 50px;
	height : 50px;
	-webkit-appearance : none;
	-moz-appearance : none;
	appearance : none;
	background-color : transparent;
	border : none;
}

.colorCss::-webkit-color-swatch{
	border-radius : 30px;
	border : none;
}
.schaduleColor{
	width : 20px;
	height : 20px;
	-webkit-appearance : none;
	-moz-appearance : none;
	appearance : none;
	background-color : transparent;
	border : none;
}

.schaduleColor::-webkit-color-swatch{
	border-radius : 30px;
	border : none;
}

.fc-h-event {
	border: none;
}

label {
    margin-bottom: -2.5rem;
}

/*오른쪽 스타일*/
#fast {
	width: 600px;
	height: 100px;
	border: 1px solid #B6B6B6;
	border-radius: 20px;
	display: flex;
	justify-content: space-evenly;
	align-items: center;
}
.fc .fc-button-primary:not(:disabled).fc-button-active, .fc .fc-button-primary:not(:disabled):active {
    background-color: #24D89E;
    border-color: #24D89E;
    color: white;
}
:root {
	--fc-button-hover-border-color: #24D59E;
	}
#pbanc, #prf, #proposal, #applicant {
	display: flex;
	flex-direction: column;
	align-items: center;
}

.fmtitle {
	font-size: 14px;
	color: #232323;
	font-weight: 500;
}

.v-line {
	border-left: 1px solid #B6B6B6;
	height: 70px;
}

#fastmenu {
	width: 600px;
	height: 110px;
	margin-top: 40px;
}

#fastmenubtn {
	display: flex;
	justify-content: space-between;
	align-items: center;
	
}

#pr, #pb, #ap, #pro {
	width: 120px;
	height: 50px;
	border: 1px solid #B6B6B6;
	border-radius: 15px;
	background: #FFFFFF;
}
#pr:hover, #pb:hover, #ap:hover, #pro:hover {
	border-color: #24D59E;
	color: #24D59E;
}
#pbancmenu{
	width: 600px;
	height: 130px;
	margin-right : 100px;
}

#scheduleBox {
	width: 400px;
	height: 130px;
	margin-right : 100px;
}

#pbanctitle, #scheduleTitle {
	display: flex;
	justify-content: space-between;
	align-items: center;
	margin-top: 20px;
	margin-bottom: 10px;
}

#pbancex{
	width: 600px;
	height: 200px;
	border-radius: 20px;
}

#scheduleEx {
	width: 400px;
	height: 200px;
	border-radius: 20px;
}

#pbanclist{
	margin: 20px auto;
	width: 600px;
	height: 185px;
	border-top: 1px solid #666363;
	border-bottom: 1px solid #666363;
	overflow-y: auto;
}

#scheduleList {
	margin: 20px auto;
	width: 400px;
	height: 185px;
	border-top: 1px solid #666363;
	border-bottom: 1px solid #666363;
	overflow-y: auto;
}

/* 인재 추천*/
