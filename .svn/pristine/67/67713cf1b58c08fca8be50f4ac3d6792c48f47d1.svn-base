/*전체*/
.registAll{
	display: flex;
	justify-content: center;
	width: 1300px;
	margin: 0px auto;
   
}
.required {
    color: red;
    margin-right: 5px;
}
/*등록 정보 전체 */
.regist{
	width: 1300px;
	padding: 20px;
}
.registTitle{
	margin-bottom: 40px;
	width: 1300px;
}

.smRegust{
	width: 1000px;
	margin: 0px auto;
	border :1px solid rgba(0, 0, 0, 0.1);
	border-radius : 0px 0px 12px 12px;
	box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
	border-top: solid 4px #24D59E;
}
.registTitle p{
	font-size: 34px;
	font: bold;
	color: #000000;
}
/*버튼*/
#editBox{
	display: flex;
    align-items: flex-end;
    margin-top:20px;
    margin-bottom: 10px;
    margin-left: 650px;
}

.cancel{
	width : 100px;
	height : 40px;
	border: 1px solid #B5B5B5;
	color: #232323;
	background:white;
	border-radius : 7px;	
	font-size : 16px;
	font-weight : 400;
	transition: all 0.3s ease 0s;
}
.cancel:hover {
	background: #ECECEC;
	border: 1px solid #B5B5B5;
}



#savebtn{
	background: white;
	color: #24D59E;
	border: 1px solid #24D59E;
	width: 170px;
	width : 100px;
	height : 40px;
	border-radius : 7px;
    font-size : 16px;
    font-weight : 400;
    margin-left:30px;
    transition: all 0.3s ease 0s;
}
#savebtn:hover {
background-color: #24D59E;
color: white;
}
#file-list {
    display: flex;
    flex-wrap:wrap;
    margin-left: 20px;
}
#file-input{
	background: white;
	color: #24D59E;
	border: 1px solid #24D59E;
	width : 100px;
	height : 40px;
	border-radius : 7px;
    font-size : 16px;
    font-weight : 400;
    margin-left:30px;
    transition: all 0.3s ease 0s;
    text-align: center;
    padding-top: 6px;
}
#file-input:hover {
background-color: #24D59E;
color: white;
}

#file-list button {
	background: white;
	color: #232323;
	border: 1px solid #B5B5B5;
	transition: all 0.3s ease 0s;
	height: 25px;
	width:25px;
	text-align: center;
	margin-left: 15px;
	margin-right: 15px;
}
#file-list button:hover {
	background: #ECECEC;
	border: 1px solid #B5B5B5;
}
#file-list {
    display: flex;
}


