/* 전형절차 그룹 */
.step-group {
    display: flex;
    flex-direction: column;
}

.step {
    display: flex;
    align-items: center;
    margin-bottom: 10px;
}

.step input {
    flex: 1;
    padding: 10px;
    margin-right: 10px;
}

.remove-step {
