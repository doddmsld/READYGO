/* 최저임금제도 모달 창 스타일 */
.modal {
	display: none; /* 기본적으로 숨김 */
	position: fixed;
	z-index: 10000;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	overflow-y: auto; 
	background-color: rgba(0, 0, 0, 0.4); /* 배경 투명도 */
}

.modal-content {
	min-height : 700px;
	max-height: 780px;
	overflow-y: auto; 
	background-color: #fefefe;
	margin: 5% auto; /* 상단에서 약간의 간격을 주고 중앙 정렬 */
	padding: 35px;
	border: 1px solid #888;
	width: 650px;
	box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0
		rgba(0, 0, 0, 0.19); /* 그림자 효과 */
}
.image-container {
  position: relative;
  display: block;
  width: 100%;
  margin-bottom: 20px;
}

.modal-content img {
  display: block;
  width: 100%;
  height: auto;
}
/* 닫기 버튼 스타일 */
.close {
	color: #aaa;
	float: right;
	font-size: 28px;
	font-weight: bold;
}

.close:hover, .close:focus {
	color: black;
	text-decoration: none;
	cursor: pointer;
}

.close:hover, .close:focus {
	color: black;
	text-decoration: none;
	cursor: pointer;
}

/* 아코디언 스타일 */
.accordion {
  background-color: #f1f1f1;
  cursor: pointer;
  text-align: left;
  border: none;
  outline: none;
  transition: 0.4s;
  width: 50px;
  height: 30px;
  border-radius: 3px;
  text-align: center;
  margin-left: 20px;
}

.accordion-content {
  display: none;
  overflow-y: auto;
  background-color: #f9f9f9;
  margin-left: 30px;
  margin-right: 30px;
  padding: 10px;
  overflow-y: auto;  /* 세로 스크롤 */
}
