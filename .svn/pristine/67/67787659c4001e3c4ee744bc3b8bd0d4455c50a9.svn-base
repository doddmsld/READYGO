    color: #ffffff;
}
/* 체크박스 그룹 스타일 */
.checkbox-group {
    display: flex;
    flex-wrap: wrap; 
}

.checkbox-button {
    display: flex;
    align-items: center;
    margin-right: 15px;
    border: 1px solid #dddddd; 
    border-radius: 4px; 
    padding: 8px 12px; 
    cursor: pointer; 
    transition: all 0.2s ease-in-out;
}

.checkbox-button input[type="checkbox"] {
    display: none; 
}

.checkbox-button label {
    font-size: 14px; 
    color: #333333;
    cursor: pointer;
}

.checkbox-button input[type="checkbox"]:checked + label {
    color: #28a745; 
    font-weight: bold; 
}

.checkbox-button input[type="checkbox"]:checked + label + .checkbox-button {
    border-color: #28a745; 
    background-color: #e6f9e8; 
}

.checkbox-button:hover {
    border-color: #cccccc;
}

.checkbox-button input[type="checkbox"]:checked + label {
    border-color: #28a745; 
    background-color: #e6f9e8; 
}
