@charset "UTF-8";
/* //////////////// confirm -> [예] 버튼  //////////////// */
.swal2-styled.swal2-confirm {
    border-radius: 5px;
    background: initial;
    background-color: #ffffff;
    border: 1px solid #24D59E;
    color: #24D59E;
    width: 60px;
    padding: 10px;
    font-size: 14px;
    border-left-color: #24d59e !important;
    border-right-color: #24d59e !important;
}
.swal2-styled.swal2-confirm:hover {
    border-radius: 5px;
    background: initial;
    background-color: #24D59E !important;
    border: 1px solid #24D59E;
    color: #ffffff;
    width: 60px;   
    padding: 10px;
    font-size: 14px;
    border-left-color: #24d59e !important;
    border-right-color: #24d59e !important;
}

/* confirm -> [아니오] 버튼 */
.swal2-styled.swal2-cancel {
    border-radius: 5px;
    background: initial;
    background-color: white;
    color: #232323;
    font-size: 14px;
    width: 60px;
    padding: 10px;
    border: 1px solid #B5B5B5;
}
.swal2-styled.swal2-cancel:hover {
    border-radius: 5px;
    background: initial;
    background-color: #ECECEC !important; 
    color: #232323;
    font-size: 14px;
    width: 60px;
    padding: 10px;
    border: 1px solid #B5B5B5;
}
.swal2-title{
   color: #232323;
    font-size: 20px;
}
.swal2-popup {
   width: 26em;
}

/* 물음표 아이콘 */
/* //////////////// question(confirm) //////////////// */
.swal2-icon.swal2-question{
   border-color: #24d59e7d;
}
.swal2-icon-content{
   color: #24d59e;
}
.swal2-icon .swal2-icon-content {
    display: flex;
    align-items: center;
    font-size: 38px;
    font-weight: 500;
}
.swal2-icon {
    position: relative;
    box-sizing: content-box;
    justify-content: center;
    width: 3em;
    height: 3em;
    margin: 1.25em auto 1.875em;
    border: 3px solid transparent;
    border-radius: 50%;
    font-family: inherit;
    line-height: 5em;
    cursor: default;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
}

/* //////////////// error(confirm) //////////////// */
.swal2-icon.swal2-error [class^=swal2-x-mark-line][class$=left] {
    left: 6px;
    transform: rotate(45deg);
}
/* sweetalert2.css 639번째 줄 */
.swal2-icon.swal2-error [class^=swal2-x-mark-line] {
    display: block;
    position: absolute;
    top: 22px;
    width: 38px;
    height: 6px;
    left: 6px;
    border-radius: 0.125em;
    background-color: #f27474;
}
.swal2-icon.swal2-error {
    border-color: #f27474 !important;
}
.swal2-icon swal2-error swal2-icon-show{
	border: 4px solid #f27474;
}

/* //////////////// warning(confirm) //////////////// */
.swal2-icon.swal2-warning {
    border-color: #ffcc54;
    color: #ffa700 !important;
}
/* XXXXXXXXXXXXX Don't touch XXXXXXXXXXXXX */
.swal2-actions:not(.swal2-loading) .swal2-styled:hover {
    background-image: linear-gradient(rgba(0, 0, 0, 0), rgba(0, 0, 0, 0));
}
