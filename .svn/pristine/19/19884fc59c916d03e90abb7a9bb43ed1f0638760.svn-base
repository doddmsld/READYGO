		<typeAlias type="kr.or.ddit.vo.OsdeCludVO" alias="osdeCludVO" /> <!-- 외주 클라우드-->	
		<typeAlias type="kr.or.ddit.vo.OsAplyVO" alias="osAplyVO" /> <!-- 외주 구입자-->	
