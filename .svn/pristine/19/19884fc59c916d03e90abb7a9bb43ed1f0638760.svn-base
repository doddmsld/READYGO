		<typeAlias type="kr.or.ddit.vo.RsmExpactEDCVO" alias="rsmExpactEDCVO" /> <!-- 활동  -->
		<typeAlias type="kr.or.ddit.vo.RsmPortfolioVO" alias="rsmPortfolioVO" /> <!-- 포트폴리오  -->
