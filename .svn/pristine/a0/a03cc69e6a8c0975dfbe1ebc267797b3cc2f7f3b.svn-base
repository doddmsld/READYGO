.member-form {
    display: flex;
    justify-content: space-around;
    flex-wrap: wrap;
    gap: 15px;
    width: 1200px;
    margin-top: 20px;
        margin-bottom: 20px;
    }
    .form-row {
        display: flex;
        align-items: flex-end;
    }
    .form-label {
        margin-right: 10px;
    }
    .required {
        color: red;
    }
    .form-input {
        width: 100%;
        padding: 8px;
        border: 1px solid #ced4da;
        border-radius: 4px;
        box-sizing: border-box;
    }
    .submit-button {
        padding: 10px 20px;
        background-color: #007bff;
        color: white;
        border: none;
        border-radius: 4px;
        cursor: pointer;
    }
    .submit-button:hover {
        background-color: #0056b3;
    }
