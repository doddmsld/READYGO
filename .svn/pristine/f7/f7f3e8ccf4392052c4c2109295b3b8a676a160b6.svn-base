@charset "UTF-8";
* {
    font-family: pretendard;
}
.row{
	flex-wrap: nowrap;
}
#topbox{
	width : 1250px;
	margin-left : 50px;
}
.box {
	padding: 15px;
    padding-left: 20px;
    border : none;
	margin : 10px;
	width : 700px;
	height : 72px;
	border-radius : 12px;
	font-size : 16px;
}
#h3{
	margin-left:-40px;
	font-weight : 700;
	font-size : 30px;
	color : #212529;
}
#count{
	font-size : 20px;
	margin-left : 68px;
	font-weight: 700;
    color: #232323;
    border-bottom: 5px solid #2CCFC3;
    padding-bottom: 5px;
    width: 90px;
}
.num, .num2 {
    font-size: 34px;
    font-weight: 700;
    text-align: right;
    margin-top: -17px;
    color : #666363;
}
.num2{
	color : #2CCFC3;
}
#fir, #fin{
	color : #666363;
	background:#F4F4F4;
	font-weight : 600;
}
#ing{
	color : #2CCFC3;
	background:#E8FAF8;
	font-weight : 600;
}
.table th{
	padding : 15px;
}

th{
	text-align : center;
	font-size : 16px;
	width : 150px;
}
thead {
    border-top: 2px solid #666363;
    border-bottom: 2px solid #666363;
}
.table thead th {
    vertical-align: bottom;
    border-bottom:none;
}
.table td {
    padding: .75rem;
    vertical-align: middle;
    border-top: 1px solid #dee2e6;
}
.card-body.table-responsive.p-0 {
    margin-left: 60px;
    width: 1230px;
}
tbody{
	text-align : center;
	font-size : 14px;
	color : #232323;
}
#total{
    background: #2CCFC3;
    color: white;
    padding: 7px 8px 7px 8px;
    border-radius: 999px;
    font-size: 20px;
}
th.propseTtl{
	 text-align:left; 
	 padding-left:110px;
}
td.propseTtl{
	 text-align:left; 
	 padding-left:110px;
	 font-size : 14px;
	 font-weight : 550;
}
th.entNm {
    text-align: left;
    padding-left: 137px;

}
td.entNm {
    text-align: left;
    padding-left: 137px;
    font-size : 14px;
	font-weight : 550;     
}
.aplcont{
	font-size : 12px;
	color : #666363;
	font-weight : normal;
	cursor: pointer;
}
td.propseDate {
    font-size : 14px;
	font-weight : 500; 
}
#okBtn{
	width: 55px;
	height: 35px;
	background: white; color : #24D59E; border : 1px solid #24D59E;
	font-size: 14px;
	font-weigth: 600;
	border-radius: 5px;
	margin-right: 5px;
	color: #24D59E;
	border: 1px solid #24D59E;	
	margin-left: 16px;
}
#okBtn:hover{
	width: 55px;
	height: 35px;
	background-color: #24D59E;
	border: 1px solid #24D59E;
	font-size: 14px;
	font-weigth: 600;
	border-radius: 5px;
	margin-right: 5px;
	color: white;
	margin-left: 16px;
}
#successBtn{
	width: 110px;
	height: 35px;
	background:rgba(44, 207, 195, 0.11);
	font-size: 14px;
	font-weigth: 600;
	border-radius: 5px;
	margin-right: 5px;
	color: #24D59E;
	border: 1px solid #24D59E;	
	margin-left: 16px;
}
#noBtn{
	width: 55px;
	height: 35px;
	border: 1px solid #B5B5B5;
	color: #232323;
	background: white;
	font-size: 14px;
	font-weigth: 600;
	border-radius: 5px;
	margin-right: 10px;
}
#noBtn:hover{
	width: 55px;
	height: 35px;
	background:#ECECEC;
	border:1px solid #B5B5B5;
	color: #232323;
	font-size: 14px;
	font-weigth: 600;
	border-radius: 5px;
	margin-right: 10px;
}
#badBtn{
	width: 110px;
	height: 35px;
	border: 1px solid #B5B5B5;
	color: #232323;
	background: white;
	font-size: 14px;
	font-weigth: 600;
	border-radius: 5px;
	margin-right: 10px;
}
span#title {
    padding: 5px 0 5px 0;
}
.modal-header {
    display: flex;
    justify-content: center;
    font-size: 26px;
    font-weight: 500;
}
/* icon */
.material-symbols-outlined {
  font-variation-settings:
  'FILL' 0,
  'wght' 200,
  'GRAD' 0,
  'opsz' 24;
   vertical-align: middle;
   font-size: 20px;  
}
@media (min-width: 670px) {
    .modal-content {
        box-shadow: 0 .5rem 1rem rgba(0, 0, 0, .5);
        height: 650px;
        width: 670px;
    }
}

/* 페이징 css 시작*/
.page-link {
    color: black; 
    border-radius: 7px; 
    margin:5px;
}
/* 버튼클릭했을 때! */
.page-item.active .page-link {
    z-index: 3;
    font-weight : 700;
    color: #24D59E;
    background-color: rgba(44, 207, 195, 0.11); 
    border-radius: 7px;
}
.write_date{
	text-align: right;
}


.page-item.active .page-link {
    border-color: #dee2e6;
}
a.page-link:hover {
    color: #000000;
    background-color: #dee2e6;
}
/* 페이징 css 끝*/



