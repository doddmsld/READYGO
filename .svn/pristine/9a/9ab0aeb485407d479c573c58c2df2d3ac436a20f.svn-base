input[type="checkbox"] {
    -webkit-appearance: none;
    position: relative;
    width: 16px;
    height: 16px;
    cursor: pointer;
    outline: none !important;
    border: 1px solid #eeeeee;
    border-radius: 2px;
    background: #fbfbfb;
}

input[type="checkbox"]::before {
    content: "\2713";
    position: absolute;
    top: 50%;
    left: 50%;
    overflow: hidden;
    transform: scale(0) translate(-50%, -50%);
    line-height: 1;
}

input[type="checkbox"]:hover {
    border-color: rgba(170, 170, 170, 0.5);
}

input[type="checkbox"]:checked {
    background-color: #24D59E;
    border-color: rgba(255, 255, 255, 0.3);
    color: white;
}

input[type="checkbox"]:checked::before {
    border-radius: 2px;
    transform: scale(1) translate(-50%, -50%)
}
/* -------------빠른 메뉴 css 시작 (끝)------------ */


/*스크롤*/
#pbanclist::-webkit-scrollbar { /* 전체 스크롤바 스타일 */
    width: 8px; /* 스크롤바의 너비 설정 */
}
#pbanclist::-webkit-scrollbar-track { /* 스크롤바의 배경 스타일 */
    background: #f1f1f1; /* 스크롤 트랙 색상 */
    border-radius: 2px; /* 트랙의 모서리 둥글게 설정 */
}
#pbanclist::-webkit-scrollbar-thumb { /* 스크롤바의 막대 스타일 */
    background: #c1c1c1; 
    border-radius: 2px; 
}
#pbanclist::-webkit-scrollbar-thumb:hover { /* 스크롤바의 막대에 마우스를 올렸을 때의 스타일 */
    background:#c1c1c1;
}

