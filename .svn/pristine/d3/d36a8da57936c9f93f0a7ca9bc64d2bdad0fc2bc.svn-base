/* 버튼 */
.btnUp, .btnsub {
   background: white;
   color: #24D59E;
   border: 1px solid #24D59E;
   padding: 8px 15px;
   margin-bottom: 10px;
   font-size: 16px;
   border-radius: 5px;
   cursor: pointer;
   width: 120px;
   height: 40px;
   transition: all 0.3s ease 0s;
}
.btnUp:hover, .btnsub:hover {
   background-color: #24D59E;
   color: white;
}
.btnDel, .btncan, .dis-btn{
   background: white;
   color: #232323;
   border: 1px solid #B5B5B5;
   padding: 8px 15px;
   margin-bottom: 10px;
   font-size: 16px;
   border-radius: 5px;
   cursor: pointer;
   width: 120px;
   height: 40px;   
   transition: all 0.3s ease 0s;
}
.dis-btn{
   width: 90px;
   height: 30px;
   font-size: 12px; 
   padding: 5px;	
}
.btnDel:hover, .btncan:hover, .dis-btn:hover {
   background: #ECECEC;
   border: 1px solid #B5B5B5;
}

.apply-btn {
   background: white;
   color: #24D59E;
   border: 1px solid #24D59E;
   padding: 8px 15px;
   margin-bottom: 10px;
   font-size: 18px;
   border-radius: 5px;
   cursor: pointer;
   width: 170px;
   height: 60px;
   transition: all 0.3s ease 0s;
}
.apply-btn:hover{
   background-color: #24D59E;
   color: white;
}		
.bookmark-btn, .top-btn  {
   background: white;
   color: #232323;
   border: 1px solid #B5B5B5;
   padding: 8px 15px;
   margin-bottom: 10px;
