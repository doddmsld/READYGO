@charset "UTF-8";

* {
    font-family: pretendard;
}
/*커서 깜빡임 없애기*/
body{
	caret-color : transparent; 
}     
.container-fluid{
	height: 1250px;
}
/*헤더 이미지*/
.headerImg{
	height: 280px;
	width: 1687px;
	margin-left:200px;
	opacity: 0.5;
}
/*헤더 이미지 위에 기업 로고 사진*/
.title{
	margin-left: 510px;  
	position:absolute; 
	top:0px; 
	left:0px; 
	z-index:1;
	margin-top: 70px;
	display: flex;
}
/*헤더 이미지 위에 기업 로고 이미지*/
.img {
    border-radius: 10px;
    height: 142px;
    width: 142px;
    object-fit: cover;
    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.3);
}
/*헤더 이미지 위에 기업명*/
h3{
	margin-left: 20px;
	margin-top: 40px;
}
/*헤더 이미지 위에 기업업종*/
#titleTpbizSeCd{
	margin-left: 20px;
}

/*기업 홈페이지 버튼*/
.titleHpBtn{
	margin-left: 270px;
	margin-top : 45px;
	height: 50px;
	width: 120px;
	border: 1px solid #A7A7A7;
	background: white;
	border-radius: 5px;
	color: #232323;
	text-align: center;
	box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
}
.titleHpBtn:hover{
	background: #ECECEC;
}
#titleGroup1{
	width: 500px;
}

/* 버튼 스타일 */
.btn {
    border: 1px solid #24D59E;
    border-radius: 5px;
    height: 40px;
    width: 120px;
    background: white;
    color: #24D59E;
    transition: background-color 0.3s ease, color 0.3s ease;
}
.btn:hover {
    background: #24D59E;
    color: white;
}   
/* 기업명 */
.comName {
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 24px;
    font-weight: bold;
    color: #333;
}

/* 버튼 정렬 */
#btndiv {
    display: flex;
    justify-content: flex-end;
    margin-top: 30px;
}

/* 프로필 정보 박스 */
#div1 {
    width: 1400px;
    margin-top: 50px;
    margin-left: 500px;
}
/* 정보 제목 */
h4{
	margin-bottom: 15px;
}
/* 정보 카드 스타일 */
#div1-2{
	display: flex;
    flex: 0 0 auto;
    padding: 20px;
    background-color: #ffffff;
    border: 1px solid rgba(217, 217, 217, 1);
    border-radius: 8px;
    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
    width: 1018px;
    line-height: 2.0rem;
}
#div1-2-2{
	margin-left: 185px;
}
/*테이블 사이 간격*/
th{
	width: 100px;
	color:#232323;
	font-weight: 550;
}
.font{
	color:#232323;
	font-weight: 550;	
}
p{
	margin: 0px;
}
/* 프로필 박스 텍스트 */
.profilebox, .profilebox1 {
    text-align: center;
    color: #5F6368;
    font-size: 16px;
}

/* 매출액 및 입사자 수 차트 영역 */
#div2 {
    width: 1200px;
    margin: 0 auto;
    margin-top: 30px;
    margin-bottom: 30px;
    margin-left: 500px;
}
#div2-2{
	margin-right: 55px;	
}
#div2-2, #div2-3 {
    width: 480px;
    background-color: #ffffff;
    padding: 20px;
    border: 1px solid rgba(217, 217, 217, 1);
    border-radius: 8px;
    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
    height: 320px;
}

/*차트 플렉스*/
.chartFlex{
	display: flex;
}
/* 차트 버튼 */
.chartbtn {
    border: 1px solid #24D59E;
    border-radius: 5px;
    height: 34px;
    background: white;
    color: #24D59E;
    transition: background-color 0.3s ease, color 0.3s ease;
}
.chartbtn:hover {
    background: #24D59E;
    color: white;
}

/* 버튼 정렬 */
#btnParent {
    margin: 10px;
    display: flex;
    justify-content: space-between;
}

/* 채용 공고 섹션 */
#div3 {
    width: 1200px;
    margin: 0 auto;
}

#div4-1 {
    display: flex;
}

/* 공고 제목 및 링크 */
#atag {
    display: flex;
    justify-content: flex-start;
    margin-top: 30px;
}
#atag1 {
	margin-top: 5px;
	margin-left: 30px;
    color: #232323;
}

#atag1:hover {
    color: #24D59E;
    text-decoration: underline;
}

/* 공고 카드 */
.entPbanc {
    flex: 0 0 auto;
    padding: 20px;
    background-color: #ffffff;
    border: 1px solid rgba(217, 217, 217, 1);
    border-radius: 8px;
    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
    height: 150px;
    width: 240px;
    margin: 7.4px;
    transition: transform 0.3s ease, background-color 0.3s ease;
}

.entPbanc:hover {
    transform: translateY(-5px);
    background-color: #f0f8f7;
}
#pbanc{
	font-size: 19px;
}
#onePbanc{
	margin-bottom: 20px;
	font-size: 14px;
}
.onePbanc1{
	font-size: 12px;
}
#onePbanc, .onePbanc1{
	color: #232323;
}
/* 모달 스타일 */
.modal {
    position: fixed;
    z-index: 1;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.4); 
    display: flex; 
    justify-content: center; 
    align-items: center; 
    
}
#ycntModal{
	position: fixed;
    z-index: 1;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.4); 
    display: flex; 
    justify-content: center; 
    align-items: center;
}


/* 모달 내부 컨텐츠 */
.modal-content {
    background-color: #fff;
    border-radius: 10px;
    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2); 
    width: 500px;
    padding: 20px;
    position: relative;
    margin: 0px auto;
    margin-top: 5%;
    border-left: 2px solid white;
    border-right: 2px solid white;
    border-top: 2px solid #24D59E;
    border-bottom: 2px solid white;     
}

/* 닫기 버튼 */
.close {
    color: #aaa;
    font-size: 24px;
    font-weight: bold;
    position: absolute;
    top: 10px;
    right: 20px;
    cursor: pointer;
}

.close:hover {
    color: #000;
}

.modal-title {
    font-size: 1.2em;
    margin-bottom: 20px;
}

.modal-buttons, #modalButtons, #ycntModalButtons{
    margin-top: 20px;
    text-align: center;
}
#editButton, #modalok1,#modalok2, #ycntEditButton, #ycntModalOk1, #ycntModalOk2{
   width : 100px;
   height : 40px;
   	border : 1px solid #24D59E;
	background: white;
	color: #24D59E;
    border-radius : 7px;
    margin-top: auto;
    font-size : 16px;
    font-weight : 400;
}

#editButton:hover, #modalok:hover
, #ycntEditButton:hover, #ycntModalOk:hover{
   background: #24D59E;
   border : none;
   color : white;
}
#registerButton, #modalcan1,#modalcan2, #ycntRegisterButton, #ycntModalCan{
   width : 100px;
   height : 40px;
   border : 1px solid #A7A7A7;
   color: #232323;
   background:white;
   border-radius : 7px;   
   font-size : 16px;
   font-weight : 400;
   margin-right: 30px;
}
#registerButton:hover, #modalcan:hover
, #ycntRegisterButton:hover, #ycntModalCan:hover {
	background: #ECECEC;
}

.modal-table {
    width: 100%;
    margin: 20px 0;
    border-collapse: collapse;
}

.modal-table th, .modal-table td {
    padding: 8px;
    border: 1px solid #ddd;
}

.input-revenue {
    width: 100%;
    padding: 8px;
    border: 1px solid #ccc;
    border-radius: 4px;
    box-sizing: border-box;
}
.input-revenue2 {
    width: 100%;
    padding: 8px;
    border: 1px solid #ccc;
    border-radius: 4px;
    box-sizing: border-box;
}
.input-revenue3 {
    width: 100%;
    padding: 8px;
    border: 1px solid #ccc;
    border-radius: 4px;
    box-sizing: border-box;
}
.input-ycnt {
    width: 100%;
    padding: 8px;
    border: 1px solid #ccc;
    border-radius: 4px;
    box-sizing: border-box;
}
/* 모달 제목 스타일 */
.modal-title {
    font-size: 1.5em;
    font-weight: bold;
    margin-bottom: 10px;
    text-align: center; 
    border-bottom: 1px solid #ddd; 
    padding-bottom: 10px;
}

.modal-subtitle {
    text-align: center;
    margin-top: 10px;
    font-size: 1em;
    color: #666;
}
.modal-waring {
    text-align: center;
    margin-top: 0px;
    font-size: 1em;
    color: #666;
}
.modal-waringy {
    text-align: center;
    margin-top: 0px;
    font-size: 1em;
    color: #666;
}
.center, .input-revenue{
	text-align: center;
}
.revenueBtn, .ycntBtn{
    border-left: 1px solid rgba(217, 217, 217, 1);
    border-right: 1px solid rgba(217, 217, 217, 1);
    border-top: 2px solid #24D59E;
    border-bottom: 1px solid white;
    background: white;  	
    height: 28px;
    text-align: center;
	margin-left: 390px;
	border-radius: 3px;
	font-size: 14px;
}
.ycntBtn{
	margin-left: 420px;	
}
/* 전체 스크롤바 스타일 */
.scroll-div::-webkit-scrollbar {
    width: 8px; /* 스크롤바의 너비 설정 */
}

/* 스크롤바의 배경 스타일 */
.scroll-div::-webkit-scrollbar-track {
    background: #f1f1f1; /* 스크롤 트랙 색상 */
    border-radius: 5px; /* 트랙의 모서리 둥글게 설정 */
}

/* 스크롤바의 막대 스타일 */
.scroll-div::-webkit-scrollbar-thumb {
    background: #c1c1c1; 
    border-radius: 5px; 
}

/* 스크롤바의 막대에 마우스를 올렸을 때의 스타일 */
.scroll-div::-webkit-scrollbar-thumb:hover {
    background:#c1c1c1;
}

.my-custom-popup {
    margin-left:128px;
}
.swal2-styled:focus {
outline: 0 !important;
box-shadow: 0 0 0 0px !important;
}
