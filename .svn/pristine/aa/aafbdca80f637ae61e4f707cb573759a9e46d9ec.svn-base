span#title {
    padding: 5px 0 5px 0;
}
.modal-header {
    display: flex;
    justify-content: center;
    font-size: 24px;
	font-weight: 600;
    background: linear-gradient(to right, #2CCFC3, #24D59E);
    color: white;
}
.modal-body2{
	padding: 13px 32px 0px 32px
}
.modal-body {
    padding: 20px 32px 0px 32px;
