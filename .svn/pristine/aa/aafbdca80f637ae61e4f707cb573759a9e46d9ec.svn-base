}
#modalpropseFile{
	border:none;
}
.modal-dialog {
    -webkit-transform: none;
    transform: none;
    position: relative;
    top: 110px;
    right:70px;
}
#control {
    display: flex;
    justify-content: flex-end;
    position: relative;
	bottom: 95px;
    right: 8px;
}


/* 버튼 */
#submitModal{
	width: 60px;
	height: 35px;
	background: white;
	font-size: 14px;
	font-weigth: 600;
	border-radius: 5px;
	margin-right: 25px;
	color: #24D59E;
	border: 1px solid #24D59E;	
}
#submitModal:hover{
	width: 60px;
	height: 35px;
	background-color: #24D59E; 
	border: 1px solid #24D59E;
	font-size: 14px;
	font-weigth: 600;
	border-radius: 5px;
	margin-right: 25px;
	color: white;	
}

#close{
	width: 60px;
	height: 35px;
	border: 1px solid #B5B5B5;
	color: #232323;
	background: white;
	font-size: 14px;
	font-weigth: 600;
	border-radius: 5px;
	margin-right: 10px;
	
}
#close:hover{
	width: 60px;
	height: 35px;
	background:#ECECEC;
	border:1px solid #B5B5B5;
	color: #232323;
	font-size: 14px;
	font-weigth: 600;
	border-radius: 5px;
	margin-right: 10px;
}



/* icon */
.material-symbols-outlined {
  font-variation-settings:
  'FILL' 0,
  'wght' 200,
  'GRAD' 0,
  'opsz' 24;
   vertical-align: middle;
   font-size: 20px;  
}
@media (min-width: 745px) {
    .modal-content {
        box-shadow: 0 .5rem 1rem rgba(0, 0, 0, .5);
        height: 745px;
        width: 690px;
		
    }
}

body.modal-open {
    padding-right: 0 !important;
}

/* 페이징 css 시작*/
.page-link {
    color: black; 
    border-radius: 7px; 
    margin:5px;
}
/* 버튼클릭했을 때! */
.page-item.active .page-link {
    z-index: 3;
    font-weight : 700;
    color: #24D59E;
    background-color: rgba(44, 207, 195, 0.11); 
    border-radius: 7px;
}
.write_date{
	text-align: right;
}


.page-item.active .page-link {
    border-color: #dee2e6;
}
a.page-link:hover {
    color: #000000;
    background-color: #dee2e6;
}
#info{
	font-size: 12px;
    line-height: 18px;
    color: #999;
    font-weight:400;
}
.apcontent{
	max-heigth:660px;
	overflow-y: auto;
}

/* 입사지원 모달 */
@media (min-width: 420px) {
    .modal-aplcontent {
        box-shadow: 0 .5rem 1rem rgba(0, 0, 0, .5);
        width: 420px;
        height: auto;
        background: white;
        border-radius: 10px 10px 0px 0px;
        position: relative;
        left: 100px;
        top: 10px;
        border: none;
    }
}
.close{
	cursor : pointer;
}
button#submit {
    width: 420px;
    height: 63px;
    margin-top: 35px;
    background: linear-gradient(to right, #2CCFC3, #24D59E);
    border: none;
    color: white;
    font-size: 16px;
}
.modal-aplheader {
    display: -ms-flexbox;
    -ms-flex-align: start;
    align-items: flex-start;
    -ms-flex-pack: justify;
    justify-content: space-between;
    padding: 1rem;
    border-top-left-radius: calc(.3rem - 1px);
    border-top-right-radius: calc(.3rem - 1px);
	padding: 30px 12px 0px 30px
}
#entScout, #aplModalEntNm{
	font-size:14px;
	color : #232323;
	border : none;
}
#aplModalpropseTtl {
    font-size: 22px;
    color: #24D59E;
    border: none;;
    font-weight: 700;
}
#propseDept{
	font-size: 22px;
    color: #24D59E;
    border: 1px solid #ddd;
    border-radius: 3px;
    font-weight: 700;
}
.fileHead {
    display: flex;
    justify-content: space-between;
    position: relative;
    padding: 4px 15px 0px 15px;
    width: 100%;
    height: 50px;
    border: 1px solid #ebebeb;
    border-bottom: 0;
    border-top-left-radius: 12px;
    border-top-right-radius: 12px;
    box-sizing: border-box;
    line-height: 56px;
    background-color: #f8f9fa;
}
.box_inner{
    position: relative;
    padding: 11px 16px 12px 16px;
    border: 1px solid #eaf0fa;
    border-radius: 12px;
    background-color: #fff;
    box-shadow: 0 2px 14px 0 rgba(0, 0, 0, .1);
    transform: translateY(-9px);
}
#headcont1,#headcont2  {
    font-size: 14px;
    font-weight: 400;
}
#headcont2{
	cursor: pointer;
}
.papericon{
	font-size:22px;
}
#paper{
	display : flex;
	gap:5px;
	color : #999;
}
#aptitle{
	font-size : 16px;
	font-weight:700;
	color: #232323;
	
}
